<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="Editor.Window.GrayColor1">#ff1f1f1f</Color>
    <Color x:Key="Editor.Window.GrayColor2">#ff262626</Color>
    <Color x:Key="Editor.Window.GrayColor3">#ff313131</Color>
    <Color x:Key="Editor.Window.GrayColor4">#ff404040</Color>
    <Color x:Key="Editor.Window.GrayColor5">#ff535353</Color>
    <Color x:Key="Editor.Window.GrayColor6">#ff6a6a6a</Color>
    <Color x:Key="Editor.Disabled.FontColor">#ff868686</Color>
    <Color x:Key="Editor.FontColor">#ffdddddd</Color>
    <Color x:Key="Editor.RedColor">#ffff5a6a</Color>
    <Color x:Key="Editor.BlueColor">#ff80deff</Color>
    <Color x:Key="Editor.GreenColor">#ff90ee90</Color>
    <Color x:Key="Editor.OrangeColor">#ffffd067</Color>
    <Color x:Key="Editor.BackgroundColor">#ff3c67b6</Color>

    <SolidColorBrush x:Key="Editor.Window.GrayColorBrush1" Color="{StaticResource Editor.Window.GrayColor1}" />
    <SolidColorBrush x:Key="Editor.Window.GrayColorBrush2" Color="{StaticResource Editor.Window.GrayColor2}" />
    <SolidColorBrush x:Key="Editor.Window.GrayColorBrush3" Color="{StaticResource Editor.Window.GrayColor3}" />
    <SolidColorBrush x:Key="Editor.Window.GrayColorBrush4" Color="{StaticResource Editor.Window.GrayColor4}" />
    <SolidColorBrush x:Key="Editor.Window.GrayColorBrush5" Color="{StaticResource Editor.Window.GrayColor5}" />
    <SolidColorBrush x:Key="Editor.Window.GrayColorBrush6" Color="{StaticResource Editor.Window.GrayColor6}" />
    <SolidColorBrush x:Key="Editor.Disabled.FontColorBrush" Color="{StaticResource Editor.Disabled.FontColor}" />
    <SolidColorBrush x:Key="Editor.FontColorBrush" Color="{StaticResource Editor.FontColor}" />
    <SolidColorBrush x:Key="Editor.RedColorBrush" Color="{StaticResource Editor.RedColor}" />
    <SolidColorBrush x:Key="Editor.BlueColorBrush" Color="{StaticResource Editor.BlueColor}" />
    <SolidColorBrush x:Key="Editor.OrangeColorBrush" Color="{StaticResource Editor.OrangeColor}" />
    <SolidColorBrush x:Key="Editor.GreenColorBrush" Color="{StaticResource Editor.GreenColor}" />
    <SolidColorBrush x:Key="Editor.BackgroundColorBrush" Color="{StaticResource Editor.BackgroundColor}" />

    <LinearGradientBrush StartPoint="0, 0" EndPoint="0, 1" x:Key="Editor.Button.NormalBackgroundBrush">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="{StaticResource Editor.Window.GrayColor5}" Offset="0"/>
            <GradientStop Color="{StaticResource Editor.Window.GrayColor4}" Offset="1"/>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush StartPoint="0, 0" EndPoint="0, 1" x:Key="Editor.Button.NormalBorderBrush">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="{StaticResource Editor.Window.GrayColor6}" Offset="0"/>
            <GradientStop Color="{StaticResource Editor.Window.GrayColor3}" Offset="1"/>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush StartPoint="0, 0" EndPoint="0, 1" x:Key="Editor.Button.HoverBackgroundBrush">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="{StaticResource Editor.BackgroundColor}" Offset="0"/>
            <GradientStop Color="#883c67b6" Offset="1"/>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush StartPoint="0, 0" EndPoint="0, 1" x:Key="Editor.Button.HoverPressBorderBrush">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="{StaticResource Editor.BackgroundColor}" Offset="0"/>
            <GradientStop Color="#883c67b6" Offset="1"/>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush StartPoint="0, 0" EndPoint="0, 1" x:Key="Editor.Button.PressBackgroundBrush">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="{StaticResource Editor.BackgroundColor}" Offset="1"/>
            <GradientStop Color="#883c67b6" Offset="0"/>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush StartPoint="0, 0" EndPoint="0, 1" x:Key="Editor.Button.DisabedBackgroundBrush">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="{StaticResource Editor.Window.GrayColor4}" Offset="0"/>
            <GradientStop Color="{StaticResource Editor.Window.GrayColor3}" Offset="1"/>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <LinearGradientBrush StartPoint="0, 0" EndPoint="0, 1" x:Key="Editor.Button.DisabledBorderBrush">
        <LinearGradientBrush.GradientStops>
            <GradientStop Color="{StaticResource Editor.Window.GrayColor5}" Offset="0"/>
            <GradientStop Color="{StaticResource Editor.Window.GrayColor2}" Offset="1"/>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
</ResourceDictionary>